<div class="row">
  <div class="offset-by-three one-third column">
    <button type="button" id="center-image">O</button>
    <button type="button" id="reset-image">X</button>
      <canvas id="canvas" style="border:solid black 1px;"></canvas>
    <form id="upload-form"
             enctype="multipart/form-data"
             action="/designer/upload"
             method="post">
          <input type="file" name="dot-design" id="dot-design"/>
