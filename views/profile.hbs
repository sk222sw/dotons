<div class="container">
  <div id="designs">
    <div class="pure-g">
      <div class="pure-u-md-1-3">&nbsp;</div>
      <div class="pure-u-1 pure-u-md-1-3">
        <h2>Profile</h2>
      </div>
      <div class="pure-u-md-1-3">&nbsp;</div>
    </div>
    {{#each designs}}
    <div class="pure-g">
      <div class="pure-u-md-1-3">&nbsp;</div>
      <div class="pure-u-1 pure-u-md-1-3 design">
        <h3>{{name}}</h2>
        <img src="" height="100px" width="100px" class="hidden" data-image-url="{{imageUrl}}" />
      </div>
      <div class="pure-u-md-1-3">&nbsp;</div>
    </div>
    {{/each}}
  
</div>
</div>


