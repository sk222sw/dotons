{
  "root": true,
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "mocha": true
  },
  "extends": "airbnb",
  "parser": "Babel-ESLint",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    "quotes": [
      0,
      "double"
    ],
    "semi": [
      "error",
      "always"
    ],
    "comma-dangle": 0,
    "prefer-template": 0,
    "func-names": 0,
    "no-console": 0,
    "new-cap": 0,
    "arrow-body-style": 0,
    "array-callback-return": 0,
    "space-before-function-paren": 0,
    "no-use-before-define": 0,
    "no-var": 0
  }
}
